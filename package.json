    "@babel/runtime-corejs2": "^7.12.17",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.2",
    "exports-loader": "^2.0.0",
    "imports-loader": "^2.0.0",
    "mini-css-extract-plugin": "^2.4.5",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.4",
    "prettier": "2.3.1",
    "terser-webpack-plugin": "^5.2.4",
    "webpack": "^5.23.0",
    "webpack-bundle-analyzer": "^4.4.0",
